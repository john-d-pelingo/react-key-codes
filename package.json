{
  "name": "react-key-codes",
  "version": "0.0.6",
  "description": "Reveal the JavaScript key codes on key down. Built with react and friends. Bootstrapped with the help of Facebook's create-react-app.",
  "private": false,
  "homepage": "https://john-d-pelingo.github.io/react-key-codes",
  "author": "John Darryl Pelingo <johndarrylpelingo@gmail.com> (https://github.com/john-d-pelingo)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/john-d-pelingo/react-key-codes.git"
  },
  "bugs": {
    "url": "https://github.com/john-d-pelingo/react-key-codes/issues"
  },
  "scripts": {
    "build": "cross-env react-scripts build",
    "build:ci": "run-s build",
    "deploy": "run-s build deploy:gh",
    "eject": "cross-env react-scripts eject",
    "lint": "eslint .",
    "nuke": "rimraf node_modules yarn.lock && yarn install",
    "postbuild:ci": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "prebuild": "run-s sweep pretty lint test",
    "prebuild:ci": "run-s sweep pretty lint test:c",
    "precommit": "lint-staged",
    "pretty": "prettier --write --config .prettierrc 'src/**/*.{js,jsx,css}'",
    "serve": "serve -so build -p 8080",
    "start": "cross-env react-scripts start",
    "sweep": "rimraf build coverage",
    "test": "cross-env CI=true react-scripts test --env=jsdom",
    "test:c": "cross-env CI=true react-scripts test --env=jsdom --coverage",
    "w:test": "cross-env react-scripts test --env=jsdom --no-cache --verbose"
  },
  "old-scripts": {
    "build": "npm run build:js",
    "build:cd": "run-s build:js copy-circle-ci-yml",
    "build:css": "node-sass --output-style expanded --indent-width 4 --linefeed lf --include-path scss src/static/scss/style.scss src/static/scss/style.css",
    "build:js": "cross-env NODE_PATH=src react-scripts build",
    "copy-circle-ci-yml": "node scripts/copy-circle-ci-yml.js",
    "deploy": "run-s build deploy:gh",
    "deploy:gh": "gh-pages -m 'Updates [ci skip]' -d build",
    "eject": "cross-env NODE_PATH=src react-scripts eject",
    "lint": "eslint .",
    "postbuild:cd": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "prebuild": "run-s build:css sweep lint test",
    "prebuild:cd": "run-s build:css sweep lint test:c",
    "start": "run-p w:css start:js",
    "start:js": "cross-env NODE_PATH=src react-scripts start",
    "sweep": "rimraf build coverage",
    "test": "cross-env NODE_PATH=src CI=true react-scripts test --env=jsdom --no-cache",
    "test:c": "cross-env NODE_PATH=src CI=true react-scripts test --env=jsdom --no-cache --coverage",
    "w:css": "nodemon -e scss -x \"npm run build:css\"",
    "w:lint": "esw . --config .eslintrc.yml --color --watch",
    "w:test": "cross-env NODE_PATH=src react-scripts test --env=jsdom --no-cache"
  },
  "keywords": [
    "create-react-app",
    "ci",
    "enzyme",
    "es6",
    "eslint",
    "jest",
    "jsx",
    "react",
    "scss"
  ],
  "dependencies": {
    "emotion": "9.1.0",
    "prop-types": "15.6.1",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-emotion": "9.1.0",
    "react-ga": "2.4.1",
    "sieses": "0.1.4"
  },
  "devDependencies": {
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.1",
    "eslint": "^4.15.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "fs-extra": "^5.0.0",
    "gh-pages": "^1.1.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.0",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.9.2",
    "react-scripts": "1.1.1",
    "react-test-renderer": "^16.2.0",
    "rimraf": "^2.6.2",
    "serve": "^6.4.4"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/*.js",
      "!src/core/*.js",
      "!src/core/**/index.js",
      "!src/views/**/index.js",
      "!<rootDir>/node_modules/"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,css,scss}": [
      "prettier --write --config .prettierrc",
      "git add"
    ]
  }
}
