{
  "name": "react-key-codes",
  "version": "0.0.5",
  "description": "Reveal the JavaScript key codes on key down. Built with react and friends. Bootstrapped with the help of Facebook's create-react-app.",
  "private": false,
  "homepage": "https://john-d-pelingo.github.io/react-key-codes",
  "author": "John Darryl Pelingo <johndarrylpelingo@gmail.com> (https://github.com/john-d-pelingo)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/john-d-pelingo/react-key-codes.git"
  },
  "scripts": {
    "build": "run-s build:js",
    "build:coverage": "run-s build:js copy-circle-ci-yml",
    "build:css": "node-sass --output-style expanded --indent-width 4 --linefeed lf --include-path scss src/views/styles/style.scss src/views/styles/style.css",
    "build:js": "react-scripts build",
    "clean": "rimraf build",
    "copy-circle-ci-yml": "node scripts/copy-circle-ci-yml.js",
    "deploy": "gh-pages  -m 'Updates [ci skip]' -d build",
    "deploy:ready": "gh-pages  -m 'Updates [ci skip]' -d build",
    "eject": "react-scripts eject",
    "lint": "eslint -c .eslintrc.yml *.js src",
    "predeploy": "npm run build",
    "prebuild": "run-s build:css clean lint test:deploy",
    "prebuild:coverage": "run-s build:css clean lint test:coverage",
    "start": "run-p watch:css start:js",
    "start:js": "react-scripts start",
    "test": "react-scripts test --env=jsdom",
    "test:deploy": "cross-env CI=true react-scripts test --env=jsdom",
    "test:coverage": "cross-env CI=true react-scripts test --env=jsdom --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "watch:css": "nodemon -e scss -x \"npm run build:css\""
  },
  "keywords": [
    "airbnb-javascript",
    "create-react-app",
    "css",
    "es6",
    "eslint",
    "eslint-config",
    "gh-pages",
    "html",
    "jsx",
    "prop-types",
    "react",
    "react-dom",
    "react-ga",
    "react-scripts",
    "redux-devtools-extension"
  ],
  "dependencies": {
    "prop-types": "15.5.10",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "react-ga": "2.2.0",
    "sieses": "0.1.4"
  },
  "devDependencies": {
    "coveralls": "2.13.1",
    "cross-env": "5.0.0",
    "enzyme": "2.8.2",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "15.0.1",
    "eslint-plugin-import": "2.3.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "7.0.1",
    "fs-extra": "3.0.1",
    "gh-pages": "1.0.0",
    "node-sass": "4.5.3",
    "nodemon": "1.11.0",
    "npm-run-all": "4.0.2",
    "react-scripts": "1.0.6",
    "react-test-renderer": "15.5.4",
    "rimraf": "2.6.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/registerServiceWorker.js",
      "!src/setupTests.js",
      "!<rootDir>/node_modules/"
    ]
  }
}
